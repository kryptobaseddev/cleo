{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "release.push request",
  "description": "Push to remote",
  "type": "object",
  "properties": {
    "version": {
      "type": "string",
      "pattern": "^\\d+\\.\\d+\\.\\d+$",
      "description": "Version"
    },
    "remote": {
      "type": "string",
      "default": "origin",
      "description": "Remote name"
    }
  },
  "required": ["version"],
  "additionalProperties": false
}
